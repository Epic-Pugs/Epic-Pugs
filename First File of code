{
let cubeSize = 5;
let width = 5;
let height = 6;
let length = 3;

player.onChat("buildBlock", function(){
  player.say("start Placing Stone");
  for (let x =1; x<= width; x++){
         for (let y =1; x<= height; y++){
             for (let z =1; z<= width; z++){
            blocks.place(BEDROCK, pos(x, y, z));
             }
         }     
     }
player.say("Done Placing Stone");
 })
